

<h1>New cocktail</h1>
<div class="col-md-3 offset-md-3">
<%= simple_form_for(@cocktail) do |f| %>
  <%= f.input :name, label: 'name' %>
  <%= f.submit "submit", class: "btn btn-primary" %>
<% end %>
</div>
<p><%= link_to 'back', :back %></p>
<p><%= link_to "All cocktails", cocktails_path, method: :get%></p>
